<div class = "forminfo">
  <h1>Meeting</h1>
  <%= form_for @meeting do |f| %>

    <div class = "spacer">
    <%= f.label :position %>
    <%= f.text_field :position %>
    </div>

    <div class = "spacer">
    <%= f.label :description %>
    <%= f.text_field :description %>
    </div>

    <div class = "spacer">
    <%= f.label :interview_num %>
    <%= f.text_field :interview_num %>
    </div>

    <div class = "spacer">
    <%= f.label :city %>
    <%= f.text_field :city %>
    </div>

    <div class = "spacer">
    <%= f.label :company_name %>
    <%= f.text_field :company_name %>
    </div>

    <div class = "spacer">
    <%= f.label :company_address %>
    <%= f.text_field :company_address %>
    </div>

    <%= f.submit %>
  <% end %>
</div>
